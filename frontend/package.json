{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "npm run test:coverage:generate && npm run test:coverage:open",
    "test:coverage:generate": "vitest run --coverage --coverage.reporter=html",
    "test:coverage:open": "start coverage/index.html"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@testing-library/vue": "^8.1.0",
    "axios": "^1.9.0",
    "pinia": "^3.0.2",
    "sass": "^1.89.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.6"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.1",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "c8": "^10.1.3",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.8"
  }
}
